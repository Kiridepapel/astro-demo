---
import Layout from '../../layouts/Layout.astro'
import { getLaunchById } from '../../services/spacex'

export const prerender = false;

const { id } = Astro.params

let launch
if (id) {
  launch = await getLaunchById(id)
}
---

<Layout title=`Lanzamiento ${id}`>
  <article>
    {launch?.id}
  </article>
</Layout>